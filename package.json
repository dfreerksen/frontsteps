{
  "title": "Frontstep Bootstrap 5 Theme",
  "name": "@dfreerksen/frontsteps",
  "license": "MIT",
  "author": "David Freerksen",
  "version": "1.0.0",
  "homepage": "https://github.com/dfreerksen/frontsteps",
  "description": "Frontstep Bootstrap 5 theme",
  "keywords": [
    "css",
    "sass",
    "html",
    "responsive",
    "theme",
    "template",
    "admin",
    "app",
    "dashboard"
  ],
  "engines": {
    "node": ">=16.14.0 <17.0.0",
    "yarn": ">=1.16.0 <2"
  },
  "scripts": {
    "build": "npm run clean && npm run build:pug && npm run build:scss && npm run build:scripts && npm run build:assets",
    "build:assets": "node scripts/build-assets.js",
    "build:pug": "node scripts/build-pug.js",
    "build:scripts": "node scripts/build-scripts.js",
    "build:scss": "node scripts/build-scss.js",
    "clean": "node scripts/clean.js",
    "start": "npm run build && node scripts/start.js",
    "start:debug": "npm run build && node scripts/start-debug.js"
  },
  "dependencies": {
    "@popperjs/core": "2.11.8",
    "bootstrap": "5.3.2"
  },
  "devDependencies": {
    "@prettier/sync": "^0.3.0",
    "Console": "^1.1.0",
    "autoprefixer": "^10.4.15",
    "browser-sync": "^2.29.3",
    "chokidar": "^3.5.3",
    "concurrently": "^8.2.1",
    "eslint": "^8.49.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "fs-extra": "^11.1.1",
    "postcss": "^8.4.29",
    "postcss-minify": "^1.1.0",
    "prettier": "^3.0.3",
    "pug": "^3.0.2",
    "sass": "^1.67.0",
    "shelljs": "^0.8.5",
    "stylelint": "^15.10.3",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^11.0.0",
    "stylelint-order": "^6.0.3",
    "uglify-js": "^3.17.4",
    "upath": "^2.0.1"
  }
}
